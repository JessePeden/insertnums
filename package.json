{
  "name": "insertnums",
  "version": "0.1.1",
  "publisher": "volkerdobler",
  "displayName": "Insert Nums",
  "description": "Insert Numbers or Alphacharts to VSCode textdocuments",
  "keywords": [
    "sequences",
    "numbers",
    "insert",
    "alphachars"
  ],
  "bugs": {
    "url": "https://github.com/volkerdobler/insertnums/issues",
    "email": "vscode.insertnums.ext@volker-dobler.de"
  },
  "homepage": "https://github.com/volkerdobler/insertnums/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/volkerdobler/insertnums.git"
  },
  "engines": {
    "vscode": "^1.40.0"
  },
  "categories": [
    "Other"
  ],
  "license": "AFL-3.0",
  "licenses": [
    {
      "type": "AFLv2.1/BSD"
    },
    {
      "type": "Apache-2.0"
    },
    {
      "type": "Artistic-2.0"
    },
    {
      "type": "BSD"
    },
    {
      "type": "BSD-2-Clause"
    },
    {
      "type": "BSD-3-Clause"
    },
    {
      "type": "CC-BY-3.0"
    },
    {
      "type": "CC-BY-4.0"
    },
    {
      "type": "CC0-1.0"
    },
    {
      "type": "GPL"
    },
    {
      "type": "ISC"
    },
    {
      "type": "LGPL"
    },
    {
      "type": "MIT"
    },
    {
      "type": "Public Domain"
    }
  ],
  "activationEvents": [
    "onCommand:extension.insertNums"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.insertNums",
        "title": "Insert Numbers"
      }
    ],
    "keybindings": [
      {
        "command": "extension.insertNums",
        "key": "ctrl+alt+.",
        "mac": "cmd+alt+.",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir out --extensions \".ts,.tsx\" --source-maps inline",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-typescript": "^7.9.0",
    "@types/vscode": "^1.43.0",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-loader": "^8.1.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-recommended": "^4.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "^2.0.4",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1"
  },
  "dependencies": {
    "@types/d3-format": "^1.3.1",
    "d3-format": "^1.4.4",
    "npm": "^6.14.4",
    "typescript-eslint": "0.0.1-alpha.0",
    "vscode": "^1.1.36"
  },
  "icon": "insertnums.png",
  "galleryBanner": {
    "color": "#5757ff",
    "theme": "dark"
  }
}
